{
  "name": "my-stack-template",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "api": "tsnd --respawn api/server.ts",
    "webapp": "vite serve webapp",
    "docker:dev": "docker compose -f ./docker-compose.dev.yml up",
    "docker:prod": "docker compose -f docker-compose.yml up",
    "build:webapp": "vue-tsc -p ./webapp/tsconfig.json && vite build webapp",
    "build:api": "tsc -p ./api/tsconfig.json",
    "build": "pnpx rimraf dist && npm run build:api && npm run build:webapp"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@vueuse/core": "^9.13.0",
    "bcrypt": "^5.1.0",
    "bulma": "^0.9.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "nanoid": "^3.3.6",
    "pinia": "^2.0.33",
    "sass": "^1.60.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vue-tsc": "^1.2.0"
  }
}
